<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Accordion?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane maxHeight="600.0" maxWidth="800.0" minHeight="600.0" minWidth="800.0" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="condo.controller.staff.RoomListController">
   <children>
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="304.0" layoutY="62.0" prefHeight="50.0" prefWidth="192.0" text="Room List">
         <font>
            <Font name="Arial Rounded MT Bold" size="30.0" />
         </font>
      </Label>
      <Button fx:id="backBtn" layoutX="375.0" layoutY="505.0" mnemonicParsing="false" onAction="#handleBackBtnOnAction" prefHeight="30.0" prefWidth="50.0" text="Back">
         <font>
            <Font name="Arial Rounded MT Bold" size="12.0" />
         </font>
      </Button>
      <TableView fx:id="residentTable" layoutX="50.0" layoutY="170.0" prefHeight="260.0" prefWidth="700.0">
        <columns>
          <TableColumn fx:id="buildingCol" maxWidth="70.0" minWidth="70.0" prefWidth="70.0" text="Building" />
          <TableColumn fx:id="floorCol" maxWidth="50.0" minWidth="50.0" prefWidth="50.0" text="Floor" />
            <TableColumn fx:id="typeCol" maxWidth="90.0" minWidth="90.0" prefWidth="90.0" text="Type" />
            <TableColumn fx:id="roomNumCol" maxWidth="90.0" minWidth="90.0" prefWidth="90.0" text="Room number" />
            <TableColumn fx:id="ownerCol" maxWidth="100.0" minWidth="100.0" prefWidth="100.0" text="Owner" />
            <TableColumn fx:id="res1Col" maxWidth="100.0" minWidth="100.0" prefWidth="100.0" text="Resident 1" />
            <TableColumn fx:id="res2Col" maxWidth="100.0" minWidth="100.0" prefWidth="100.0" text="Resident 2" />
            <TableColumn fx:id="res3Col" maxWidth="100.0" minWidth="100.0" prefWidth="100.0" text="Resident 3" />
        </columns>
      </TableView>
      <Button fx:id="refreshBtn" layoutX="650.0" layoutY="447.0" mnemonicParsing="false" onAction="#handleRefreshBtnOnAction" prefHeight="30.0" prefWidth="100.0" text="Refresh">
         <font>
            <Font name="Arial Rounded MT Bold" size="12.0" />
         </font></Button>
      <Label layoutX="50.0" layoutY="447.0" prefHeight="30.0" prefWidth="350.0" text="Double click on the room to &quot;Add resident&quot;">
         <font>
            <Font name="Arial Rounded MT Bold" size="15.0" />
         </font>
      </Label>
      <Accordion />
   </children>
</AnchorPane>
