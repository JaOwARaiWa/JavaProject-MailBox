<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="condo.controller.ChangePasswordController">
   <children>
      <Label layoutX="200.0" layoutY="101.0" prefHeight="340.0" prefWidth="400.0" style="-fx-border-color: black;" />
      <Label layoutX="224.0" layoutY="101.0" prefHeight="100.0" text="Change Password">
         <font>
            <Font name="Mitr Medium" size="40.0" />
         </font>
      </Label>
      <Label layoutX="249.0" layoutY="193.0" text="Username" />
      <Label layoutX="249.0" layoutY="279.0" text="New Password" />
      <Label layoutX="249.0" layoutY="321.0" text="Comfirm Password" />
      <Label fx:id="progressLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="251.0" layoutY="400.0" prefHeight="30.0" prefWidth="300.0" textAlignment="CENTER">
         <font>
            <Font name="Mitr Medium" size="15.0" />
         </font>
      </Label>
      <PasswordField fx:id="oldPassField" layoutX="400.0" layoutY="232.0" />
      <PasswordField fx:id="newPassField" layoutX="401.0" layoutY="275.0" />
      <PasswordField fx:id="confirmField" layoutX="401.0" layoutY="317.0" />
      <Button fx:id="changeBtn" layoutX="372.0" layoutY="375.0" mnemonicParsing="false" onAction="#handleChangeBtnOnAction" text="Change" />
      <Button fx:id="backBtn" layoutX="380.0" layoutY="483.0" mnemonicParsing="false" onAction="#handleBackBtnOnAction" text="Back" />
      <Label layoutX="249.0" layoutY="236.0" text="Old Password" />
      <Label fx:id="usernameLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="400.0" layoutY="185.0" prefHeight="25.0" prefWidth="149.0">
         <font>
            <Font name="Mitr Medium" size="15.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
